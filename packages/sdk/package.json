{
  "name": "lunarcrush-comprehensive-sdk",
  "version": "2.0.1",
  "description": "Comprehensive TypeScript SDK for LunarCrush with 100% GraphQL backend coverage - 2000% improvement over limited versions",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "vitest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "lunarcrush",
    "crypto",
    "social",
    "intelligence",
    "graphql",
    "sdk",
    "typescript",
    "comprehensive",
    "universal",
    "sentiment",
    "analytics"
  ],
  "author": "dbatson09",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/danilobatson/lunarcrush-universal.git",
    "directory": "packages/sdk"
  },
  "homepage": "https://github.com/danilobatson/lunarcrush-universal#readme",
  "bugs": {
    "url": "https://github.com/danilobatson/lunarcrush-universal/issues"
  },
  "devDependencies": {
    "@types/node": "^20.0.0",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "@lunarcrush/shared-types": "workspace:*"
  },
  "engines": {
    "node": ">=18"
  }
}
