{
  "name": "lunarcrush-sdk",
  "version": "3.1.1",
  "description": "TypeScript/JavaScript SDK for LunarCrush API v4 with GraphQL support",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist/**/*",
    "README.md",
    "package.json"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "lint": "tsc --noEmit",
    "test": "vitest",
    "prepublishOnly": "npm run lint && npm run build",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "lunarcrush",
    "crypto",
    "social",
    "sentiment",
    "api",
    "graphql",
    "typescript",
    "sdk"
  ],
  "author": "LunarCrush",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-repo/lunarcrush-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-repo/lunarcrush-sdk/issues"
  },
  "homepage": "https://lunarcrush.com",
  "dependencies": {
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "lunarcrush-sdk": "file:lunarcrush-sdk-3.1.1.tgz"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "directories": {
    "example": "examples"
  }
}
